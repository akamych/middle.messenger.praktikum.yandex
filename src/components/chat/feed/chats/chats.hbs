<li class="feedChat" data-chat-id={{ id }}>
    <div class="avatar">
    {{#if avatar }}
        <img src="{{ avatar }}" />
    {{/if}}
        
    </div>
    <b>{{ title }}</b>
    <p>
        {{#if last_message }}
            {{ last_message.content }}
        {{/if}}
    </p>
    {{#if unread_count }}
        <span class="new">{{ unread_count }}</span>
    {{/if}}
</li>
